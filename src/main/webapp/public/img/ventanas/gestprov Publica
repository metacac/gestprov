CREATE TABLE IF NOT EXISTS `gestprov`.`invoices` (
  `idFromInvoice` bigint unsigned NOT NULL, 
  `toInvoice` bigint unsigned NOT NULL,
  `operationNum` bigint unsigned NOT NULL,
  `invoiceNum` bigint unsigned NOT NULL, 
  `payTotal` bigint unsigned NOT NULL, 
  `dateIssue` timestamp NOT NULL,
  `dateExpire` timestamp NOT NULL,
  `status` varchar(255) NOT NULL,
  INDEX (`idFromInvoice`),
  FOREIGN KEY (`idFromInvoice`) REFERENCES `gestprov`.`customer`(`cuit`)
);


