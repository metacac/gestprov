CREATE DATABASE  IF NOT EXISTS `gestprov` CHARACTER SET utf8 COLLATE utf8_general_ci;
USE `gestprov`;

CREATE TABLE IF NOT EXISTS `gestprov`.`customer` (
  `id` mediumint unsigned NOT NULL AUTO_INCREMENT, // no puede existir un AUTO_INCREMENT y otro PRIMARY KEY distinto, lo resolvemos luego si es necesario //
  `date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `cuit` bigint unsigned NOT NULL,
  `email` varchar(255) NOT NULL,
  `namesRepre` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `celular` bigint unsigned NOT NULL,
  `password` varchar(255) NOT NULL,
  PRIMARY KEY (`id`)
);

