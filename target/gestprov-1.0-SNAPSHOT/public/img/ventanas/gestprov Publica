CREATE TABLE IF NOT EXISTS `gestprov`.`invoices` (
  `idInvoice` mediumint unsigned NOT NULL AUTO_INCREMENT,
  `fromInvoice` varchar(255) NOT NULL, 
  `toInvoice` varchar(255) NOT NULL,
  `operationNum` varchar(255) NOT NULL,
  `invoiceNum` varchar(255) NOT NULL, 
  `payTotal` bigint unsigned NOT NULL, 
  `dateIssue` timestamp NOT NULL,
  `dateExpire` timestamp NOT NULL,
  `statusInvoice` varchar(255) NOT NULL,
  PRIMARY KEY (`idInvoice`),
  INDEX (`toInvoice`),
  FOREIGN KEY (`toInvoice`) REFERENCES `gestprov`.`information`(`cuit`) ON UPDATE CASCADE ON DELETE CASCADE
);

SET FOREIGN_KEY_CHECKS=0; //PARA PODER CARGAR LOS DATOS QUE SE AUTOCONTROLAN

