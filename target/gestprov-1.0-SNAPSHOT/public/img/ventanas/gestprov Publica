
CREATE DATABASE  IF NOT EXISTS `gestprov` CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci;
USE `gestprov`;

CREATE TABLE IF NOT EXISTS `gestprov`.`information` (
  `idCustomer` mediumint unsigned NOT NULL AUTO_INCREMENT,
  `cuitInf` varchar(255) NOT NULL UNIQUE,
  `namesCust` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `companyName` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `phone` varchar(255) NOT NULL,
  `direction` varchar(255) NOT NULL,
  `postalCode` varchar(255) NOT NULL,
  `city` varchar(255) NOT NULL,
  `country` varchar(255) NOT NULL,
  `statusCustomer` varchar(255) DEFAULT "No certificado" NOT NULL,
  `typeCust` varchar(255) NOT NULL,
  PRIMARY KEY (`idCustomer`,`cuit`),
);

