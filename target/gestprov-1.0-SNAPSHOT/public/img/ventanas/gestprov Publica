CREATE TABLE IF NOT EXISTS `gestprov`.`customer` (
  `id` mediumint unsigned NOT NULL AUTO_INCREMENT,
  `date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `cuitCus` varchar(255) NOT NULL,
  `email` varchar(255) NOT NULL,
  `namesRepre` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `celular` varchar(255) NOT NULL,
  `password` varchar(255) NOT NULL,
  PRIMARY KEY (`id`),
  INDEX (`cuit`),
  FOREIGN KEY (`cuit`) REFERENCES `gestprov`.`information`(`cuit`) ON UPDATE CASCADE
);

